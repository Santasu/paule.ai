{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs22.x",
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "rewrites": [
    { "source": "/api/paule/v1/:path*", "destination": "/api/:path*" },
    { "source": "/wp-json/paule/v1/:path*", "destination": "/api/:path*" },
    { "source": "/augam/v1/:path*", "destination": "/api/:path*" }
  ],
  "cleanUrls": true
}

