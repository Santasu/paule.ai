{
  "version": 2,
  "framework": null,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "^/$", "dest": "/index.html" },
    { "src": "^(?!/api)(?!/_next)(?!/assets)(?!/spec)(?!/favicon\\.ico)(?!/robots\\.txt)(/.*)$", "dest": "/index.html" }
  ]
}
